@using Flags.Models
@using Models
@model QuestionViewModel



    <h1>Please choose flag for @Model.QuestionText</h1>

    <div class="row">
        @foreach (FlagViewModel flag in Model.FlagChoices)
        {
        <div class="col-md-4">
            <img id="@(flag.ID)" class="flagImg" src="~/Images/FlagsGuid/@(flag.ID).png" />
        </div>

        }
    </div>



    <script>
    @foreach (FlagViewModel flag in Model.FlagChoices)
    {

        @:$('#@flag.ID').click(function () { console.log("@flag.ID") });

    }

    </script>

