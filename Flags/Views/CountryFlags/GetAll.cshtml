@using Models
@model IEnumerable<IEnumerable<CountryFlag>>



@foreach (IEnumerable<CountryFlag> row in Model)
{
<div class="row">
    @foreach (CountryFlag chunklet in row)
    {
        <div class="col-md-4">
            @(chunklet.Description)
            
                <img class="flagImg" src="~/Images/Flags/@(chunklet.CountryCode).png" />
    </div>
    }
    
</div>
}




